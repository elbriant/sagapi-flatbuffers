
enum Torappu_CharMasterType : int {
	NONE = 0,
	SYSTEM = 1,
	BATTLE = 2
}

table Torappu_CharMasterLevelData {
	level:int;
	name:string;
	description:string;
	conditionDesc:string;
}



table Torappu_CharMasterBasicData {
	charId:string;
	masterId:string;
	sortId:int;
	masterType:Torappu_CharMasterType;
	levelList:[Torappu_CharMasterLevelData];
}



root_type Torappu_CharMasterBasicData;